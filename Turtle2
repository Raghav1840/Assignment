#make turtle Outerline
from turtle import*

timmy = Turtle()
timmy.shape("turtle")
timmy.color("#86469C")

def drawSquare():
    timmy.forward(200)
    timmy.left(90)
    timmy.forward(200)
    timmy.left(90)
    timmy.forward(200)
    timmy.left(90)
    timmy.forward(200)

drawSquare()
timmy.fillcolor("#83C8CF")
screen = Screen()
print(screen.canvheight)
screen.exitonclick()
